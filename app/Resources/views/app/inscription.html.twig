{% extends 'base.html.twig' %}



{% block body %}

    <h1>Inscription</h1>

    {% for type, messages in app.flashes %}
        {% for message in messages %}
            <div class="alert alert-{{ type }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}

    <div class="form-group">
        {{ form(formUser) }}
    </div>


{% endblock %}